<plist version="1.0">
<dict>
    <!-- Chiave NSPrincipalClass al livello principale -->
    <key>NSPrincipalClass</key>
    <string>$(PRODUCT_MODULE_NAME).SampleHandler</string>

    <!-- Chiave NSExtension con le sue proprietÃ  -->
